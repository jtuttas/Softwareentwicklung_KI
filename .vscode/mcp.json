{
    "servers": {
        "dbhub-mysql-npx": {
            "command": "npx",
            "args": [
                "-y",
                "@bytebase/dbhub",
                "--transport",
                "stdio",
                "--dsn",
                "mysql://root:@localhost:3306/bsp_pcshop"
            ]
        },
        "MCP SQLite Server": {
            "type": "stdio",
            "command": "npx",
            "args": [
                "-y",
                "mcp-sqlite",
                "database.sqlite"
            ]
        }
    }
}